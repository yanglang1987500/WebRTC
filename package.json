    "cs-channel": "^1.0.4",
    "js-cookie": "^2.2.0",
    "lodash-es": "^4.17.15",
    "mobx": "^4.6.0",
    "mobx-react": "^5.3.6",
    "moment": "^2.24.0",
    "photoswipe": "^4.1.3",
    "qs": "^6.6.0",
    "rc-animate": "^2.6.0",
    "rc-calendar": "^9.13.2",
    "rc-dialog": "^7.3.0",
    "rc-form": "^2.4.3",
    "rc-progress": "^2.5.2",
    "react": "^16.8.4",
    "react-animate-height": "^2.0.15",
    "react-custom-scrollbars": "^4.2.1",
