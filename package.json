    "react-pdf": "^4.0.5",
    "react-phone-input-auto-format": "^0.1.0",
    "react-photoswipe": "^1.3.0",
    "react-player": "^1.11.0",
    "react-router-dom": "^5.0.0",
    "react-slick": "^0.24.0",
    "react-toast-notifications": "^2.2.5",
    "react-transition-group": "^1.2.1",
    "sweetalert2": "^8.5.0",
    "video-react": "^0.14.1"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>\\fe-toolkit"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.less?$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "setupFilesAfterEnv": [
      "./node_modules/jest-enzyme/lib/index.js",
      "<rootDir>/__mocks__/setupTests.ts"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "enzyme",
      "jest-enzyme"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|mess|less)$": "identity-obj-proxy",
      "axios": "<rootDir>/__mocks__/axios.ts",
      "^@store(.*)$": "<rootDir>/src/store$1",
      "^@business(.*)$": "<rootDir>/src/business$1",
      "^@common(.*)$": "<rootDir>/src/common$1",
      "^@pages(.*)$": "<rootDir>/src/pages$1",
      "^@containers(.*)$": "<rootDir>/src/containers$1",
      "^@router(.*)$": "<rootDir>/src/router$1",
      "@components": "<rootDir>/src/components/index.ts",
      "^@mocks(.*)$": "<rootDir>/__mocks__$1",
      "@http": "<rootDir>/src/common/utils/http.ts"
    }
  },
  "pre-commit": {
    "run": [
      "lint"
    ]
  },
  "sideEffects": [
    "*.css",
    "*.less"
  ]
}
