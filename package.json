{
  "name": "react-spa-cli",
  "version": "1.0.0",
  "description": "react-spa-cli",
  "main": "index.js",
  "scripts": {
    "dll": "./node_modules/.bin/webpack --mode production --config ./build/webpack.dll.config.js",
    "cleanChunks": "./node_modules/.bin/gulp cleanChunks -f ./build/gulpfile.js",
    "dev": "./node_modules/.bin/webpack-dev-server --mode development --config ./build/webpack.config.js --open",
    "webpack": "./node_modules/.bin/webpack --mode production --config ./build/webpack.config.js",
    "build": "npm run cleanChunks && npm run webpack",
    "lint": "./node_modules/.bin/tslint -c tslint.json 'src/**/*.ts' 'src/**/*.tsx'",
    "lint:fix": "./node_modules/.bin/tslint --fix -c tslint.json 'src/**/*.ts' 'src/**/*.tsx'",
    "service": "cd ./mocks && node -r esm app.js",
    "server": "tsc -p ./server && nodemon --inspect --exec node ./server/index.js",
    "test": "jest --coverage -u",
    "font": "node ./build/font/font-generator.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
